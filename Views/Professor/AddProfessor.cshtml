@model University.Models.ProfessorModel
@{
    ViewBag.Title = "Добавить преподавателя";
}


    <form method="post">
        <div>
            <label asp-for="Id">Уникальный идентификатор: </label><br />
            <input asp-for="Id" />
            <span asp-validation-for="Id"></span>
        </div>
        <div>
            <label asp-for="CathedraId">Номер кафедры: </label><br />
            <input asp-for="CathedraId" />
            <span asp-validation-for="CathedraId"></span>
        </div>
        <div>
            <label asp-for="StartDate">Дата начала работы: </label><br />
            <input asp-for="StartDate" />
            <span asp-validation-for="StartDate"></span>
        </div>
        <div>
            <label asp-for="Surname">Фамилия: </label><br />
            <input asp-for="Surname" />
            <span asp-validation-for="Surname"></span>
        </div>
        <div>
            <label asp-for="Name">Имя: </label><br />
            <input asp-for="Name" />
            <span asp-validation-for="Name"></span>
        </div>
        <div>
            <label asp-for="Patronymic">Отчество: </label><br />
            <input asp-for="Patronymic" />
            <span asp-validation-for="Patronymic"></span>
        </div>
        <div>
            <label asp-for="WorkPosition">Должность: </label><br />
            <input asp-for="WorkPosition" />
            <span asp-validation-for="WorkPosition"></span>
        </div>
        <div>
            <label asp-for="AcademicDegree">Научное звание: </label><br />
            <input asp-for="AcademicDegree" />
            <span asp-validation-for="AcademicDegree"></span>
        </div>
        <div>
            <label asp-for="Other">Дополнительные сведения: </label><br />
            <input asp-for="Other" />
            <span asp-validation-for="Other"></span>
        </div>
        <div><input type="submit" value="Отправить" /></div>
    </form>